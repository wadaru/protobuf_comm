{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "set_gamephase command schema",
    "description": "This commands sets the gamephase to the given phase, must be all caps.",
    "default": {},
    "examples": [
        {
            "command": "set_gamephase",
            "phase": "PRODUCTION"
        }
    ],
    "required": [
        "command",
        "phase"
    ],
    "additionalProperties": true,
    "properties": {
        "command": {
            "$id": "#/properties/command",
            "type": "string",
            "default": "",
            "examples": [
                "set_gamephase"
            ]
        },
        "phase": {
            "$id": "#/properties/phase",
            "type": "string",
            "default": "",
            "examples": [
                "PRODUCTION"
            ]
        }
    }
}
